{
	"name": "FastFind",
	"short_name": "FF",
	"description": "FastFind helps you track of pieces of text!",
	"version": "0.1",
	"author": "Samuel Nunes",
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},

	"content_scripts": [
		{
			"persistent": true,
			"js": ["findAndReplaceDOMText.js", "index.js"],
			"matches": ["<all_urls>"],
			"run_at": "document_end"
		}
	],
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"minimum_chrome_version": "60",
	"manifest_version": 2
}
